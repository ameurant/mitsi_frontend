<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="fr"
      xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:metal="http://xml.zope.org/namespaces/metal"
      xmlns:i18n="http://xml.zope.org/namespaces/i18n"
      lang="fr"
      metal:use-macro="context/main_template/macros/master"
      i18n:domain="box">

<body onLoad="document.insertBoxPrelevement.box-qrcode.focus();">

<metal:block fill-slot="top_slot"
           tal:define="dummy python:request.set('disable_border',1)" />

<metal:main fill-slot="content-core"
            tal:define="boxesView context/@@manageBox;
                        labsView context/@@manageLabs;
                        allLabs labsView/getListingLabs">

    <section class="formulaire">
        <h2>
            <i class="fas fa-ellipsis-v"></i>
            Relever une boite
        </h2>
        <form action="insertBoxPrelevement"
              method="post">
            <div class="form-input">
                <label for="box-qrcode">
                    QRCode la boite :
                </label>
                <input type="text"
                       name="boxQRCode"
                       id="box-qrcode"
                       value="" />
            </div>
            <div class="formControls" align="center">
                <input type="submit"
                       name="submit"
                       value="Enregister" />
            </div>
        </form>
    </section>
</metal:main>
</body>
</html>
